{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": [".env"],
  "globalDotEnv": [".env"],
  "pipeline": {
    "lint": {},
    "dev": {
      "cache": false,
      "persistent": true,
      "dotEnv": [".env"]
    },
    "backend#build": {
      "dependsOn": ["frontend#build", "^build"]
    },
    "frontend#build": {
      "dependsOn": ["^build"]
    },
    "shared-code#build": {}
  }
}
